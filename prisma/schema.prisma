generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Job {
  id         String    @id
  owner      String
  pattern    String
  deployer   String
  state      String
  salt       String?
  address    String?
  createdAt  DateTime  @default(now())
  finishedAt DateTime?
}
